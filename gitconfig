# http://githowto.com/
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p
        unstage = reset HEAD
        us = reset HEAD
        diffs = diff --ignore-space-change
        alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
[color]
        ui = true
[push]
        default = current
